{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks":
 {
    "clean": { "cache": false },
    "dev": { "cache": false, "persistent": true	},
    "build": { "dependsOn": ["^build"], "outputs": ["dist/**", ".next/**", "build/**"]	},
    "lint": { "dependsOn": ["^lint"] },
    "test": { "dependsOn": ["^test"], "outputs": ["coverage/**"] },
    "typecheck": { "dependsOn": ["^typecheck"] }
  }
}
